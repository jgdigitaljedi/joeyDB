{
  "name": "joeydb",
  "version": "1.0.0",
  "description": "graphql API using MongoDB, JWT, Apollo, and Express",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc || echo done.",
    "tsc:prod": "tsc -p tsconfig.production.json || echo done.",
    "build": "npm-run-all clean tsc",
    "build:prod": "npm-run-all clean tsc:prod",
    "start": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start:env": "NODE_ENV=development node --require dotenv/config dist/main JoeyDB",
    "start:prod": "NODE_ENV=production node dist/main JoeyDB",
    "dev:start": "npm-run-all build start:env",
    "clean": "rimraf dist/*",
    "watch": "ts-node-dev src/main.ts",
    "stop": "pkill --signal SIGINT JoeyDB",
    "seed:platforms": "node seed/platforms.js",
    "seed:users": "node seed/users.js --all=$ALL",
    "seed:all": "ALL=true npm-run-all build seed:users seed:platforms"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgdigitaljedi/joeyDB.git"
  },
  "keywords": [
    "graphql",
    "JWT",
    "mongoDB",
    "express",
    "typescript"
  ],
  "author": "Joey Gauthier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jgdigitaljedi/joeyDB/issues"
  },
  "homepage": "https://github.com/jgdigitaljedi/joeyDB#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.1",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "apicalypse": "^0.1.3",
    "apollo-server-express": "^2.5.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "chalk": "^2.4.2",
    "express": "^4.17.0",
    "express-graphql": "^0.8.0",
    "express-jwt": "^5.3.1",
    "graphql": "^14.3.0",
    "graphql-s2s": "^0.20.0",
    "graphql-tools": "^4.0.4",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.5.9",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  }
}